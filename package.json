{
  "name": "appriver-official-site",
  "version": "1.0.0",
  "description": "appriver公式サイト - SUNO AIで制作した楽曲の配信サイト",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint *.js",
    "lint:fix": "eslint --fix *.js",
    "verify": "npm run format:check && npm run lint",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kazcchi/appriver-official-site.git"
  },
  "keywords": [
    "music",
    "streaming",
    "suno-ai",
    "appriver"
  ],
  "author": "appriver",
  "license": "MIT",
  "engines": {
    "node": ">=20.18.0"
  },
  "packageManager": "npm@10.8.2",
  "devDependencies": {
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}
